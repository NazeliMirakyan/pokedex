{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "*": [
        "../../.."
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "project/redux/index.jsx",
    "pages/index.tsx",
    "project/containers/header/logo/Logo.jsx",
    "project/containers/header/logo/Logo.jsx",
    "project/containers/homeContainer/HomeContainer.jsx",
    "src/components/sort/Filter.tsx",
    "src/components/content/Content.tsx",
    "src/components/header/logo/Logo.tsx",
    "src/containers/pokemonPage/homeContainer/HomeContainer.tsx",
    "src/components/sort/limitFilter/LimitFilter.tsx",
    "src/components/sort/typeFilter/TypeFilter.tsx",
    "src/redux/data/slice.tsx",
    "src/redux/index.tsx",
    "src/redux/data/selectors.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}